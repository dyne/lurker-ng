SubDir TOP render ;
SubDirHdrs $(TOP) libesort ;
SubDirHdrs $(TOP) common ;

Main lurker.cgi :
	main.cpp
	art.cpp
	url.cpp
	mailto.cpp
	quote.cpp
	attach.cpp
	mbox.cpp
	message.cpp
	mindex.cpp
	search.cpp
	splash.cpp
	thread.cpp
	Cache.cpp
	KeyReader.cpp
	Summary.cpp
	Threading.cpp
	;

LinkLibraries lurker.cgi : libesort libcommon ;

Main jump.cgi :
	parse.cpp
	jump.cpp
	;

Main keyword.cgi :
	parse.cpp
	keyword.cpp
	;

LinkLibraries keyword.cgi : libcommon ;

InstallBin $(LIBDIR)/cgi-bin : 
	lurker.cgi 
	jump.cgi 
	keyword.cgi
	;
